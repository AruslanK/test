<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="KandyTests" parallel="false">
   <test name="Registration" preserve-order="true">
       
       <listeners>
       		<listener class-name="uitl.ListenerOne"/>	
       </listeners>

       <groups>
       <define name="all">
           <include name="functest"/>
           <include name="checkintest"/>
       </define>
       <run>
           <include name="all" />
           <exclude name="failed" />
       </run>
   </groups>
		
       <parameter name="testBrowser" value="Chrome"/> 
       
       <classes>
            <class name="RegistrationTest.NewTest" />
       </classes>

    </test>
</suite>